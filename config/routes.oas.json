  "paths": {
    "/ws/echo": {
      "x-zuplo-path": {
        "pathMode": "open-api"
      },
      "get": {
        "summary": "Web Socket Test",
        "description": "Lorem ipsum",
        "x-zuplo-route": {
          "corsPolicy": "none",
          "handler": {
            "export": "webSocketPipelineHandler",
            "module": "$import(@zuplo/runtime)",
            "options": {
              "rewritePattern": "https://www.julian-labs.com/websocket",
              "policies": {
                "inbound": [
                  {
                    "export": "inboundFirewallPolicy",
                    "module": "$import(./modules/ws-inbound-firewall)"
